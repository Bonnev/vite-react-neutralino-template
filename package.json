{
	"name": "vite-project",
	"version": "1.0.0-alpha",
	"scripts": {
		"start": "concurrently -k -n vite-dev,neu \"yarn:vite-dev\" \"neu run -- --url=http://127.0.0.1:5173/index.html --document-root=\"\\/./\"\"",
	"build": "vite build --mode prod && neu build",
		"preview": "vite build --mode prod && neu run -- --url=http://localhost:8080/index.html",
		"vite-dev": "vite --mode dev",
		"vite-preview": "vite preview"
	},
	"dependencies": {
		"moment": "^2.29.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"vite-plugin-html-env": "^1.2.4"
	},
	"devDependencies": {
		"@neutralinojs/neu": "^9.3.1",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@vitejs/plugin-react": "^2.0.1",
		"concurrently": "^7.3.0",
		"eslint": "^8.23.0",
		"eslint-plugin-react": "^7.31.4",
		"vite": "^3.0.7"
	}
}
